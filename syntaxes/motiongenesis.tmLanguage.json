{
    "name": "MotionGenesis",
    "scopeName": "source.motiongenesis",
    "fileTypes": [
        "txt",
        "al"
    ],
    "patterns": [
        {
            "name": "comment.source.motiongenesis",
            "match": "(%.*)"
        },
        {
            "name": "storage.source.motiongenesis",
            "match": "(^NewtonianFrame|^RigidBody|^RigidFrame|^Particle|^Point|^Variable|^Constant|^Specified|^Input|^OutputPlot|^Output)"
        },
        {
            "name": "keyword.source.motiongenesis",
            "match": "(=)"
        },
        {
            "name": "keyword.source.motiongenesis",
            "begin": "\\[",
            "end": "\\]",
            "patterns": [
                {
                    "name": "constant.numeric.motiongenesis",
                    "match": "(\\d+)"
                }
            ]
        },
        {
            "name": "constant.numeric.motiongenesis",
            "match": "(( |\\.|-)(\\d+))"
        },
        {
            "include": "#all_motionGen_function"
        }
    ],
    "repository": {
        "all_motionGen_function": {
            "patterns": [
                {
                    "include": "#motionGen_mass_function"
                },
                {
                    "name": "support.function.motiongenesis",
                    "match": "(RotateX|RotateNegativeY)"
                }
            ]
        },
        "motionGen_mass_function": {
            "name": "keyword.source.motiongenesis",
            "match": "(GetMass|SetMass|GetCharge|SetCharge|GetCmPosition|GetCmVelocity|GetCmAcceleration|GetInertiaDyadic|GetInertiaMatrix|GetMomentOfInertia|GetProductOfInertia|GetRadiusOfGyration|GetInertiaVector|SetInertia)"
        }
    }
}